{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "sourceMap": false,
    "allowJs": true,
    "pretty": true,
    "declaration": false,
    "module": "ES2015",
    "moduleResolution": "node",
    "preserveSymlinks": true,
    "importHelpers": false,
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "target": "ES5",
    "skipLibCheck": true,
    "lib": [],
    "paths": {
      "react-unity-renderer": [
        "node_modules/react-unity-renderer/index.ts"
      ],
      "src/*": [
        "./src/*"
      ],
      "*": [
        "./node_modules/*",
        "./node_modules/@types/*"
      ]
    }
  },
  "files": [
    "./index.ts",
    "./index.d.ts",
    "./typings.d.ts"
  ],
  "exclude": [
    "./dist/**/*.*"
  ]
}
